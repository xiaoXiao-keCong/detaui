{
  "name": "detaui",
  "version": "0.0.0",
  "scripts": {
    "dev": "yarn lint && yarn vite ",
    "build:web": "vue-tsc --noEmit && vite build --outDir web --base /detaui/ --emptyOutDir --config ./vite.doc.config.ts",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint ./src --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts  --fix --ignore-path .gitignore",
    "gen": "npx plop --plopfile ./scripts/generate-component/plopfile.js"
  },
  "dependencies": {
    "lerna": "^4.0.0",
    "normalize.css": "^8.0.1",
    "rollup-plugin-esbuild": "^4.8.2",
    "rollup-plugin-vue": "^6.0.0",
    "vite-plugin-vuedoc": "^3.1.3",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.25",
    "@vitejs/plugin-vue": "^2.2.2",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "plop": "^3.0.5",
    "prettier": "^2.6.0",
    "typescript": "~4.5.5",
    "vite": "^2.8.4",
    "vue": "^3.2.31",
    "vue-tsc": "^0.31.4"
  }
}
